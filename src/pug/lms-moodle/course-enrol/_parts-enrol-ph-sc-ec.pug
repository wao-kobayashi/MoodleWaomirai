script.
  $(document).ready(function() {
      // タブのクリックイベントを設定
      $('.enrol-section-basesubject-year-lesson-tab-child').click(function() {
          // すべてのタブとコンテンツを非アクティブにする
          $('.enrol-section-basesubject-year-lesson-tab-child').removeClass('active');
          $('.enrol-section-basesubject-year-lesson-content-child').hide(); // コンテンツを非表示にする
          
          // クリックされたタブをアクティブにし、対応するコンテンツを表示する
          $(this).addClass('active');
          var targetContentId = '#content-level-' + $(this).attr('id').split('-')[2]; // タブのID末尾（level番号）でコンテンツIDを取得
          $(targetContentId).show(); // 対応するコンテンツを表示する
      });
      
      // 最初に表示するタブを選択（初期状態）
      $('.enrol-section-basesubject-year-lesson-tab-child:first').click();
  });